    "paths": {
        "/public-v1/company": {
            "get": {
                "summary": "Get all companies",
                "operationId": "e78fc27e3c25464f00243ca09b5ead2e",
                "parameters": [
                    {
                        "name": "search",
                        "in": "query",
                        "description": "Search term",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of companies"
                    }
                }
            },
            "post": {
                "summary": "Create a new company",
                "operationId": "f1213fd903fa5b64d19fb84e46e1ddb1",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CompanyRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Company created successfully"
                    },
                    "400": {
                        "description": "Invalid input"
                    }
                }
            }
        },
        "/public-v1/company/{id}": {
            "get": {
                "summary": "Get a company by ID",
                "operationId": "150cb123826f440e277d3c19222d17e2",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the company",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Company details"
                    },
                    "404": {
                        "description": "Company not found"
                    }
                }
            },
            "put": {
                "summary": "Update a company",
                "operationId": "b173e965cc424a13369f6db329500715",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the company to update",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CompanyRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Company updated successfully"
                    },
                    "404": {
                        "description": "Company not found"
                    }
                }
            },
            "delete": {
                "summary": "Delete a company",
                "operationId": "479420027a8cbaded5132d5810e4599f",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the company to delete",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Company deleted successfully"
                    },
                    "404": {
                        "description": "Company not found"
                    }
                }
            }
        },
        "/public-v1/company/delete-bulk": {
            "post": {
                "summary": "Bulk delete companies",
                "operationId": "c26f507c91ebfaa905aa7cb3ac1de110",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "ids": {
                                        "description": "Array of company IDs to delete",
                                        "type": "array",
                                        "items": {
                                            "type": "integer"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "202": {
                        "description": "Companies deleted successfully"
                    }
                }
            }
        },
