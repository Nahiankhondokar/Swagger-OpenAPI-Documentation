    "components": {
        "schemas": {
            "CompanyRequest": {
                "required": [
                    "name",
                    "email"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "maxLength": 191
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "phone_number": {
                        "type": "string",
                        "nullable": true
                    },
                    "website": {
                        "type": "string",
                        "format": "url",
                        "maxLength": 191,
                        "nullable": true
                    },
                    "c_size": {
                        "type": "integer",
                        "nullable": true
                    },
                    "c_type": {
                        "type": "integer",
                        "nullable": true
                    },
                    "country_id": {
                        "type": "integer",
                        "nullable": true
                    },
                    "city_id": {
                        "type": "integer",
                        "nullable": true
                    },
                    "state_id": {
                        "type": "integer",
                        "nullable": true
                    },
                    "address": {
                        "type": "string",
                        "nullable": true
                    }
                },
                "type": "object"
            },
            "ContactRequest": {
                "required": [
                    "first_name",
                    "email"
                ],
                "properties": {
                    "first_name": {
                        "type": "string",
                        "maxLength": 191
                    },
                    "company_id": {
                        "type": "integer",
                        "nullable": true
                    },
                    "last_name": {
                        "type": "string",
                        "maxLength": 191,
                        "nullable": true
                    },
                    "phone_code": {
                        "type": "string",
                        "maxLength": 5,
                        "nullable": true
                    },
                    "phone_number": {
                        "type": "string",
                        "maxLength": 18,
                        "nullable": true
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "country_id": {
                        "type": "integer",
                        "nullable": true
                    },
                    "state_id": {
                        "type": "integer",
                        "nullable": true
                    },
                    "city_id": {
                        "type": "integer",
                        "nullable": true
                    },
                    "address": {
                        "type": "string",
                        "nullable": true
                    },
                    "image": {
                        "type": "integer",
                        "nullable": true
                    },
                    "zip_code": {
                        "type": "string",
                        "maxLength": 8,
                        "nullable": true
                    },
                    "timezone": {
                        "type": "string",
                        "maxLength": 191,
                        "nullable": true
                    },
                    "access_portal": {
                        "type": "integer",
                        "enum": [
                            0,
                            1
                        ],
                        "nullable": true
                    },
                    "password": {
                        "type": "string",
                        "maxLength": 191,
                        "minLength": 6,
                        "nullable": true
                    },
                    "position": {
                        "type": "string",
                        "nullable": true
                    },
                    "custom_fields": {
                        "type": "array",
                        "items": {},
                        "nullable": true
                    },
                    "invite_client": {
                        "type": "boolean",
                        "nullable": true
                    }
                },
                "type": "object"
            }
        }
    }
